<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Profile Page</title>
  </head>
  <body>
    <%- include('common/header') %>

    <h1>Welcome <%= user.name %></h1>
    <h1><%= user.email %></h1>
    <h1><%= user.city %></h1>
    <ul>
      <% user.skills.forEach((skill)=>{ %>
      <li><%= skill %></li>
      <% }) %>
    </ul>
  </body>
</html>
